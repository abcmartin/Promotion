	note = {1532-1827 Maeda, S Shinchi, H Kurahara, H Mataki, Y Maemura, K Sato, M Natsugoe, S Aikou, T Takao, S Journal Article Research Support, Non-U.S. Gov't 2008/03/20 Br J Cancer. 2008 Apr 22;98(8):1389-97. doi: 10.1038/sj.bjc.6604307. Epub 2008 Mar 18.},
	number = {8},
