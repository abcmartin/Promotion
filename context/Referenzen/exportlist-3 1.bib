	note = {Franceschi, S Talamini, R Barra, S Bar{\'o}n, A E Negri, E Bidoli, E Serraino, D La Vecchia, C Journal Article Research Support, Non-U.S. Gov't United States 1990/10/15 Cancer Res. 1990 Oct 15;50(20):6502-7.},
	number = {20},
	pages = {6502-7},
