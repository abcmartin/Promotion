	note = {1930-0573 Bagnardi, V Blangiardo, M La Vecchia, C Corrao, G Journal Article Meta-Analysis Research Support, Non-U.S. Gov't 2002/03/26 Alcohol Res Health. 2001;25(4):263-70.},
	number = {4},
	pages = {263-70},
	title = {Alcohol consumption and the risk of cancer: a meta-analysis},
