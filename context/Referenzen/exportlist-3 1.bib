	number = {6},
	pages = {172-93},
	title = {Role of human papillomavirus in oropharyngeal squamous cell carcinoma: A review},
	type = {Journal Article},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4061306/pdf/WJCC-2-172.pdf},
