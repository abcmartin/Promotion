	note = {Jakob, Mark Sharaf, Kariem Schirmer, Markus Leu, Martin Kuffer, Stefan Bertlich, Mattis Ihler, Friedrich Haubner, Frank Canis, Martin Kitz, Julia eng Germany Strahlenther Onkol. 2021 Mar;197(3):231-245. doi: 10.1007/s00066-020-01653-5. Epub 2020 Jun 25.},
	number = {3},
