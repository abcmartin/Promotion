	number = {3},
	pages = {231-245},
	title = {Role of cancer stem cell markers ALDH1, BCL11B, BMI-1, and CD44 in the prognosis of advanced HNSCC},
	type = {Journal Article},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7892527/pdf/66_2020_Article_1653.pdf},
	volume = {197},
