	issn = {0020-7136},
	journal = {Int J Cancer},
	keywords = {Adult Aged Aged, 80 and over Carcinoma, Squamous Cell/*etiology/*mortality Female Humans Male Middle Aged Papillomaviridae/classification/*genetics Prognosis *Smoking Tonsillar Neoplasms/*etiology/*mortality},
