	note = {1474-5488 Marur, Shanthi D'Souza, Gypsyamber Westra, William H Forastiere, Arlene A R01 DE021395/DE/NIDCR NIH HHS/United States Journal Article Review 2010/05/11 Lancet Oncol. 2010 Aug;11(8):781-9. doi: 10.1016/S1470-2045(10)70017-6. Epub 2010 May 5.},
	number = {8},
